<%= form_for Listing.new, :html => {:class => "navbar-form navbar-left"}, :url => search_path, role: "search", method: :get do |f| %>
	<%= text_field_tag :term, params[:term],placeholder:"Where are you going?", :style => "width: 170px" %>
	<%= f.submit 'Location Search', :class => "btn btn-default" %>
<% end %>